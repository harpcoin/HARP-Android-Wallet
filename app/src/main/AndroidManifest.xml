<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.harpbnbwalletsample">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/harp_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    android:screenOrientation="portrait"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ImportByPrivateKeyActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".CheckBalanceActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".ExportPrivateKeyActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".ImportFromKeyStoreActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".ExportKeyStoreActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".CreateWalletActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".SendBNBActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".CheckBEP20TokenBalanceActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
        <activity
            android:name=".SendBEP20TokenActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            />
    </application>

</manifest>